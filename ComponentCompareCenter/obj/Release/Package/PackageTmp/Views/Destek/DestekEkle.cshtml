
@model ComponentCompareCenter.Models.Siniflar.Destek
@{
    ViewBag.Title = "DestekEkle";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}


<br />
<h2>Destek Ekleme Sayfası</h2>
<br />

<form class="form-group" method="post">

    @Html.LabelFor(x => x.sorular)
    @Html.TextBoxFor(x => x.sorular, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.cevaplar)
    @Html.TextBoxFor(x => x.cevaplar, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>

</form>


